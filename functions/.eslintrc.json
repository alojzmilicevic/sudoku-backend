{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-var": "off",
    "one-var": [
      "off"
    ],
    "no-alert": "off",
    "no-console": "off",
    "consistent-return": "off",
    "prefer-destructuring": "off",
    "no-plusplus": "off"
  }
}
